{
  "name": "daruma-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "serve": "npm run-script build && npm run start && firebase emulators:start --only functions",
    "shell": "npm build && firebase functions:shell",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "deploy": "firebase deploy --only functions,firestore",
    "start:dev": "nest start --watch",
    "start:test": "env NODE_ENV=test nest start --watch --debug",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "precypress:test": "pm2 start npm -- run start:test",
    "cypress:test": "cypress run",
    "postcypress:test": "pm2 delete npm",
    "cypress:open": "cypress open -d"
  },
  "dependencies": {
    "@firebase/database": "^0.5.24",
    "@firebase/functions": "^0.4.38",
    "@nestjs/common": "^7.0.7",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.7",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.7",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/swagger": "^4.5.1",
    "@types/mongoose": "^5.7.8",
    "@types/uuid": "^7.0.2",
    "@types/uuid-validate": "0.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.5.0",
    "geteventstore-promise": "^3.2.1",
    "lint": "^0.7.0",
    "mongoose": "^5.13.15",
    "nestjs-config": "^1.4.5",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "shallow-equal-object": "^1.1.1",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.1.4",
    "tslint": "^6.1.0",
    "uuid": "^7.0.2",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.7",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.8",
    "@types/passport-http-bearer": "^1.0.35",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cypress": "^4.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.2.4",
    "pm2": "^4.2.3",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
